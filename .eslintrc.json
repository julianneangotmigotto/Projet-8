{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-undef": "off",
        "no-trailing-spaces": "off",
        "no-unused-vars": "off",
        "indent": "off"
    }
}